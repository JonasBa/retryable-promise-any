{
  "name": "retryable-promise-race",
  "version": "0.0.0",
  "repository": {
    "type": "git"
  },
  "author": "Jonas Badalic <jonas@badalic.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "build:examples": "yarn run build && yarn run examples:copy",
    "clean": "rm -rf ./dist && rm -rf examples/src/retry",
    "examples:start": "yarn --cwd ./examples start",
    "examples:copy": "mkdir examples/src/retry && cp -r dist/ examples/src/retry",
    "lint": "eslint 'lib/**/*.ts'",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.11",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  }
}
